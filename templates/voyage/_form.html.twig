{{ form_start(form) }}
    {{ form_row(form.destination) }}
    {{ form_row(form.description) }}
    <div class="row">
        <div class="col">
            {{ form_row(form.startAt) }}
        </div>
        <div class="col">
            {{ form_row(form.endAt) }}
        </div>
    </div>
    {{ form_row(form.status) }}
    {{ form_row(form.users) }}
    <div class="text-end">
        <button class="btn btn-indigo">{{ button_label|default('Save') }}</button>
    </div>
{{ form_end(form) }}
